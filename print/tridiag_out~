a = [2 2 2 2 2]

a =

     2     2     2     2     2

b= [-1 -1 -1 -1 -1]

b =

    -1    -1    -1    -1    -1

c = [-1 -1 -1 -1 -1]

c =

    -1    -1    -1    -1    -1

f= [1 0 0 0 1]

f =

     1     0     0     0     1

my_trisolve(a,b,c,f)

ans =

     9.999999999999999e-01
     9.999999999999998e-01
     9.999999999999999e-01
     1.000000000000000e+00
     1.000000000000000e+00

format short e
my_trisolve(a,b,c,f)

ans =

   1.0000e+00
   1.0000e+00
   1.0000e+00
   1.0000e+00
   1.0000e+00

diary off
